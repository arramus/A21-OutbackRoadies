<configs>

<!--APPOC CAR-->
<append xpath="/nav_object_classes">
<nav_object_class name="apoccar" extends="bicycle">
	<property name="requirement_type" value="IsVehicleOwner" />

	<compass_settings>
		<property name="sprite_name" value="ui_game_symbol_apoccar"/>
	</compass_settings>

	<onscreen_settings>
		<property name="sprite_name" value="ui_game_symbol_apoccar"/>
	</onscreen_settings>
</nav_object_class>
</append>
<!--END OF APPOC CAR-->

