<configs>

	<!-- Edit player entity to increase inventory size - KhaineGB -->
	<set xpath="/entity_classes/entity_class[@name='playerMale']/effect_group/passive_effect[@name='CarryCapacity'][@value='27']/@value">40</set>
	<set xpath="/entity_classes/entity_class[@name='playerMale']/effect_group/passive_effect[@name='BagSize'][@value='45']/@value">60</set>

	<!-- Load out kit with Preppocalypse influence -->
	<set xpath="/entity_classes/entity_class[contains(@name,'playerMale')]/property[contains(@name,'GameModeSurvival')]/@value">OutbackRoadiesStarterKit,StarterRewardPocoCar,medicalFirstAidBandage,meleeToolTorch,keystoneBlock,npcAnimalFoxStarterFood</set>	
	<set xpath="/entity_classes/entity_class[contains(@name,'playerMale')]/property[contains(@name,'GameModeSurvivalSP')]/@value">OutbackRoadiesStarterKit,StarterRewardPocoCar,medicalFirstAidBandage,meleeToolTorch,keystoneBlock,npcAnimalFoxStarterFood</set>	
	<set xpath="/entity_classes/entity_class[contains(@name,'playerMale')]/property[contains(@name,'GameModeSurvivalMP')]/@value">OutbackRoadiesStarterKit,StarterRewardPocoCar,medicalFirstAidBandage,meleeToolTorch,keystoneBlock,npcAnimalFoxStarterFood</set>

