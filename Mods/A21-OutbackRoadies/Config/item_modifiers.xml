	<item_modifier name="modDyeAussieGold" installable_tags="clothing,armor,weapon,tool,vehicle,drone" modifier_tags="dye" type="attachment" cosmetic_install_chance=".16">
		<property name="Extends" value="modGeneralMaster"/>
